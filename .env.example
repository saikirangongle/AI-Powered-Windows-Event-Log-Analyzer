# .env.example
# Copy this file to `.env` and fill in your values.
# Do NOT commit real secrets to version control.

# Gemini API key (required)
GEMINI_API_KEY=your_gemini_api_key_here

# Application mode: development | production
APP_ENV=development

# Logging level: DEBUG | INFO | WARNING | ERROR
LOG_LEVEL=INFO

# Optional: default path to look for logs (can be overridden in app settings)
DEFAULT_LOG_DIR=

# Timeout for Gemini HTTP requests (seconds)
GEMINI_TIMEOUT=25

# Optional: override Gemini base URL (useful for staging/proxy)
GEMINI_BASE_URL=https://api.gemini.example
